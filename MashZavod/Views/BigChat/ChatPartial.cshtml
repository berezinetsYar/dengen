<ul class="chat">
    @foreach (var b in ViewBag.Chat)
    {
    <li class="left clearfix">
        <span class="chat-img pull-left">
            <img src="http://placehold.it/50/55C1E7/fff&text=U" alt="User Avatar" class="img-circle" />
        </span>
        <div class="chat-body clearfix">
            <div class="header">
                <strong class="primary-font">@b.LoginUs</strong> <small class="pull-right text-muted">
                    <span class="glyphicon glyphicon-time"></span>@b.Time_message
                </small>
            </div>
            <p>@b.Text_message</p>
        </div>
    </li>
    }
</ul>


