<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>News</title>
</head>
<body>
    <div>
        @foreach(var elem in ViewBag.listNews)
        {
            <div>
                <a href="@elem.Link">@elem.Title</a>
                <p>@elem.Description</p>
                <p>@elem.PubDate Релевантность - @elem.Relevance</p>
                <a href="@elem.Source">@elem.Source</a>
            </div>
            <br>
        }
    </div>
    
</body>
</html>
<div class="row">
    @Html.Action("Index", "BigChat")
</div>
